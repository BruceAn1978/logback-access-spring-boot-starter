<!DOCTYPE configuration>

<configuration>
    <statusListener class="ch.qos.logback.core.status.OnConsoleStatusListener" />
    <filter class="net.rakugakibox.spring.boot.logback.access.test.RequestHeaderDrivenLogbackAccessFilter" />
    <appender
        name="IN_MEMORY_QUEUE"
        class="net.rakugakibox.spring.boot.logback.access.test.InMemoryLogbackAccessEventQueueAppender" />
    <appender-ref ref="IN_MEMORY_QUEUE" />
</configuration>
